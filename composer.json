{
    "name": "basaltinc/twig-renderer",
    "description": "Render templates using Twig PHP, via this Node JS renderer.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BasaltInc\\": "dist/"
        }
    },
    "config": {
        "platfrom": {
            "php": "8.2"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "react/http": "^1.10.0",
        "twig/twig": "^v3.14.2"
    },
    "require-dev": {},
    "sort-packages": true
}
